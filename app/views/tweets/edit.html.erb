<%= form_for @tweet do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, :size => 140 %> 
   <%= f.label :recipe %>
  <%= f.text_field :recipe, :size => 140 %> 
   <%= f.label :material %>
  <%= f.text_field :material, :size => 140 %> 
   <%= f.label :time %>
  <%= f.text_field :time, :size => 140 %> 
   <%= f.label :comment %>
  <%= f.text_field :comment, :size => 140 %> 
   <%= f.label :image %>
  <%= f.text_field :image, :size => 140 %> 
  <%= f.submit "編集する" %>
<% end %>